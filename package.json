{
  "name": "fr-script",
  "private": true,
  "version": "2.0.7",
  "type": "module",
  "env": "development",
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "dev": "node .\\script\\switchEnv && vite",
    "play":"vite --mode play",
    "play:build":"vite build --mode play",
    "build": "vue-tsc --noEmit && node .\\script\\switchEnv ERROR &&vite build",
    "build:debug": "vue-tsc --noEmit && node .\\script\\switchEnv DEBUG &&vite build",
    "preview": "vite preview",
    "tauri": "pnpm genAll && tauri dev",
    "tauri:build": "pnpm genAll && pnpm build && tauri build",
    "tauri:build:debug": "pnpm build:debug && tauri build",
    "tauri:only_build": "tauri build",
    "tauri:only_build_d": "node .\\script\\switchEnv DEBUG && tauri build",
    "genType": "node .\\script\\genBuiltInApiType.js",
    "genDeclare": "node .\\script\\genApiDeclare.js",
    "genEnum": "node .\\script\\genEnumDeclare.js",
    "genAll": "pnpm genEnum && pnpm genType && pnpm genDeclare",
    "verify": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@tauri-apps/api": "1.6.0",
    "@vueuse/core": "11.0.3",
    "animate.css": "4.1.1",
    "axios": "1.7.7",
    "crypto-js": "4.2.0",
    "element-plus": "2.8.3",
    "highlight.js": "11.10.0",
    "monaco-editor": "0.52.0",
    "nanoid": "5.0.7",
    "pinia": "2.2.2",
    "ts-morph": "23.0.0",
    "typescript": "^5.6.2",
    "vue-draggable-plus": "0.5.3",
    "vue-hooks-plus": "2.2.1",
    "vue-router": "4.4.3"
  },
  "devDependencies": {
    "@iconify-json/bxl": "1.2.0",
    "@iconify-json/ep": "1.2.0",
    "@iconify-json/mdi": "1.2.0",
    "@iconify-json/solar": "1.2.0",
    "@iconify-json/svg-spinners": "1.2.0",
    "@tauri-apps/cli": "1.6.1",
    "@types/crypto-js": "4.2.2",
    "@types/node": "22.5.3",
    "@unocss/preset-icons": "0.62.3",
    "@vitejs/plugin-vue": "5.1.3",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue-hooks-plus/resolvers": "1.2.5",
    "@vue/test-utils": "^2.4.6",
    "chalk": "5.3.0",
    "jsdom": "^25.0.0",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "1.78.0",
    "unocss": "0.62.3",
    "unplugin-auto-import": "0.18.2",
    "unplugin-icons": "0.19.2",
    "unplugin-vue-components": "0.27.4",
    "unplugin-vue-macros": "^2.12.2",
    "vite": "^5.4.6",
    "vitest": "^2.1.1",
    "vue": "3.5.0",
    "vue-tsc": "2.1.6"
  }
}